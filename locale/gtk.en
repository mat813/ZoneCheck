        <tag name="local">local resolver</tag>
        <tag name="local/tip">allow selection of an alternative local resolver</tag>
      </section>

      <section name="metatest">
         <tag name="mail">mail delivery</tag>
         <tag name="mail/tip">performs tests involving messagerie</tag>
         <tag name="zone">zone content</tag>
         <tag name="zone/tip">performs zone transfert tests</tag>
         <tag name="rir">RIR databases</tag>
         <tag name="rir/tip">performs tests about IP addresses space</tag>
      </section>

    <tag name="expert/output/format">Select output format</tag>
