        <tag name="udp">UDP</tag>
        <tag name="udp/tip">Use UDP protocol for all queries</tag>
        <tag name="tcp">TCP</tag>
        <tag name="tcp/tip">Use TCP protocol for all queries</tag>
      </section>

      <section name="resolver">
      </section>
