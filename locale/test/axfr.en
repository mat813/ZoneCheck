[chk_axfr]
testname: zone transfert possible
error   : Zone transfert not possible
explain : [none]

[chk_axfr_empty]
testname: zone transfert contains data
error   : Zone transfert is empty
explain : [none]

[chk_axfr_valid_labels]
testname: zone transfert contains valid labels
error   : Zone transfert containts invalid label(s)
explain = label_xpl

